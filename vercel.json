{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "server.js"
    },
    {
      "src": "/(.+\\.css)",
      "dest": "server.js"
    },
    {
      "src": "/(.+\\.js)",
      "dest": "server.js"
    },
    {
      "src": "/(.+\\.html)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}
